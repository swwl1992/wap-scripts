{
  "manifest_version": 2,
  "name": "GitLab Rebuild",
  "version": "1.0",
  "description": "A scheduler for CI rebuild of gitlab",
  "browser_action": {
    "default_icon": "favicon.png",
    "default_title": "GitLab Rebuild",
    "name": "Click me"
  },
  "permissions": [
    "tabs",
    "alarms",
    "http://*/*",
    "https://*/*"
  ],
  "background":{
    "scripts": [
      "background.js"
    ]
  }
}
